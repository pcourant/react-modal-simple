{
  "name": "react-modal-simple-customizable",
  "version": "1.0.0",
  "description": "Demo page for react-modal-simple-customizable",
  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.css dist/esm",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn copy-files",
    "build2": "parcel build src/index.html --dist-dir docs --public-url .",
    "start": "parcel src/index.html",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pcourant/react-modal-simple-customizable.git"
  },
  "author": "Pierre Courant (pcourant)",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.8.2",
    "process": "^0.11.10",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.4",
    "typescript-plugin-css-modules": "^4.1.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-code-blocks": "^0.0.9-0",
    "react-dom": "^18.2.0",
    "react-modal-simple-customizable": "^1.0.3",
    "react-transition-group": "^4.4.5"
  }
}
